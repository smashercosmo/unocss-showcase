{
  "name": "unocss-showcase",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint:fix": "pnpm run lint --fix",
    "prettier": "prettier --check '**/*.*(md|json|yaml|html|yml)' --log-level warn",
    "prettier:fix": "pnpm run prettier --write",
    "test": "vitest"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@unocss/preset-mini": "^66.3.3",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.2.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "unocss": "^66.3.3",
    "vite": "^7.0.4",
    "vitest": "^3.1.1",
    "jsdom": "^25.0.1"
  }
